<div class="col-block col-md-auto resume-picture-holder text-center text-md-start">
    @if (ViewBag.Image == null)
    {
        <img class="picture" src="~/Themes/CvTheme/assets/images/profile.jpg" style="height: 220px;">
    }
    else
    {
        <img class="picture" src="~/UserImageFiles/@ViewBag.Image" style="height: 220px; width:220px;">
    }
</div>
